[user]
	name = Andrey Shvetsov
	email = andrey.shvetsov@k2l.de
[push]
	default = simple
[alias]
	l = log --oneline --graph --decorate
	ll = log --oneline --graph --decorate --all
	ls = log --oneline --graph --decorate --simplify-by-decoration
	lls = log --oneline --graph --decorate --simplify-by-decoration --all
	l3 = log --oneline --graph --decorate -33
	ll3 = log --oneline --graph --decorate -33 --all
	st = status -s
	ci = commit
	co = checkout
	di = diff
[core]
	editor = vim
